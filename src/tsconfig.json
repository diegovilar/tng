{
    "compilerOptions": {
        "target": "ES5",
        "module": "commonjs",
        "noEmitOnError": true,
        "noImplicitAny": true,
        "emitDecoratorMetadata": true,
        "preserveConstEnums": true,
        "removeComments": false,
        "experimentalDecorators": true,

        "outDir": "./build",
        "declaration": false,
        "sourceMap": false
    }
}