{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "noEmitOnError": true,
        "declaration": true,
        "stripInternal": false,
        "suppressImplicitAnyIndexErrors": false,
        "experimentalDecorators": true,
        "sourceMap": true,
        "newLine": "LF",
        "charset": "utf-8",
        "emitBOM": false,
        "lib": [
            "dom",
            "es5"
        ],
        "baseUrl": ".",
        "paths": {
            "angularts.core": ["./core/index"],
            "angularts.ui.bootstrap": ["./ui.bootstrap/index"],
            "angularts.ui.router": ["./ui.router/index"]
        }
    },
    "exclude": [
        "all/*"
    ]
}